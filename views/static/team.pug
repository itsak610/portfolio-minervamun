extends layout 

block content
    style.
        section.s8{
            height:100vh;
            background-image: linear-gradient(to bottom, rgba(0,0,0, 0.2),rgba(0,0,0, 0.2)), url("https://www.pitblado.com/wp-content/uploads/2020/09/pitblado-people.jpg");
            background-repeat: no-repeat;
            background-size: cover;
            /* background-attachment: fixed; */
            backdrop-filter: blur(30px);
            background-color: #000;
        }
        .leftside{
            float:left;
            text-align:left;
            margin-left:150px;
            color:#fff;
            font-size:2vw;
            margin-top:50vh
        }
        .leftside p{
            font-size: 2vw;
        }
        .leftside b{
            text-decoration: underline;
        }
        .titlediv{
            display:flex;
            flex-flow:row;
        }
        .title{
            padding-right:2vw;
        }
        section.s9{
            height:40vh;
        }
        section.s9 .leftside{
            color:#697881;
            margin-top:17vh;
        }
        section.s9 .leftside b{
            color:#000;
        }
        section.s10{
            height:max-content;
        }
        .imagetab1{
            margin-top:20vh;
            display:flex;
            flex-flow:row;
            justify-content: space-evenly;
            align-content:center;
            width:100vw;
        }
        .imagediv{
            width:20vw;
        }
        .imagepart img{
            width:18vw;
            height:18vw;
        }
        .imagepart img:hover{
            opacity: 0.5;        
        }
        .textpart h1{
            font-size:2em;
        }
        .textpart p{
            letter-spacing:0.001em;
            color: #697881;
            font-size:1.2em;
        }
        @media (min-width:1000px) and (max-width:1300px) {
            .textpart h1{
                font-size:1.6em;
            }
            .textpart p{
                font-size:1em;
            }
        }

        @media (min-width:750px) and (max-width:1000px) {
            .textpart h1{
                font-size:1.3em;
            }
            .textpart p{
                font-size:0.8em;
            }
        }

        @media (max-width:750px) {
            h1.title{
                font-size:8vh;
                text-align:left;
                margin-top:-50px;
            }
            .titlediv{
                margin-top:20vh;
                flex-flow:column;
            }
            .leftside{
                margin-left:20px;
            }
            .leftside p{
                font-size:1.6vh;
                line-height:2.8vh
            }
            .textpart h1{
                font-size:1.3em;
            }
            .textpart p{
                font-size:0.9em;
            }
            .imagetab1{
                flex-flow:column;
                align-items:center;
            }
            .imagepart img{
                margin-top:10vh;
                width:30vh;
                height:30vh;
            }
            .imagediv{
                width:30vh;
            }
            .imagetab1:nth-child(2), .imagetab1:nth-child(3){
                margin-top:0;
            }
        }

