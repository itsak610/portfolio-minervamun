                rzp1.open();
                e.preventDefault();

                return false;
            }
            // Otherwise, display the correct tab:
            showTab(currentTab);
            }

            function validateForm() {
            // This function deals with validation of the form fields

            var x, y, i, valid = true;
            x = document.getElementsByClassName("tab");
            y = x[currentTab].getElementsByTagName("input");
            z = x[currentTab].getElementsByTagName("select");
            for (i in userList){
                if (document.getElementById('emailinput').value == userList[i].email){
                    document.getElementById('emailinput').className += " invalid";
                    document.getElementById('wronginput1').classList.add('wronginput1')
                    valid = false;
                }
            }
            for (i in userList){
                if (document.getElementById('numberinput').value == userList[i].number){
                    document.getElementById('numberinput').className += " invalid";
                    document.getElementById('wronginput2').classList.add('wronginput2')
                    valid = false;
                }
            }
            // A loop that checks every input field in the current tab:
            for (i = 0; i < y.length; i++) {
                if (y[i].value == "") {
                    // add an "invalid" class to the field:
                    y[i].className += " invalid";
                    // and set the current valid status to false:
                    valid = false;
                }
            }
            for (i = 0; i < z.length; i++) {
                if (z[i].value == ""  || z[i].value == 'SelectState' || z[i].value == 'SelectCity' || z[i].value == 'SelectCommittee') {
                    // add an "invalid" class to the field:
                    z[i].className += " invalid";
                    // and set the current valid status to false:
                    valid = false;
                }
            }
            if (currentTab == 1){
                if ($('.allotdiv2')[0].childElementCount == 0){
                    document.getElementById('commitvalue').className += ' invalid'
                    valid = false;
                    document.getElementById('allotmenttext').classList.add('textreveal1')
                }            
            }
            // If the valid status is true, mark the step as finished and valid:
            if (valid) {
                document.getElementsByClassName("step")[currentTab].className += " finish";
            }
            return valid; // return the valid status
            }

