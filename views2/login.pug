            margin-top:-4em;
            }

            .card1 {
            height: 100%;
            }

            .content {
            font-size: 30px;
            }

            .card2 {
            width: 80%;
            height: 80px;
            }

            .content {
            height: 100%;
            width: 100%;
            display: flex;
            justify-content: space-evenly;
            align-items: center;
            flex-flow: column;
            }

            .content p {
            letter-spacing: 1px;
            font-size: 35px;
            font-weight: 100;
            text-align: center;
            }
            .js-tilt-glare{
                border-radius:15px;
            }
            /* -------------------------------------------------CARD-CONTAINER------------------------------------------------- */

            .card-container {
                height: 100vh;
                width: 100%;
                display: flex;
                color: #fff;
                justify-content: space-evenly;
                align-items: center;
                position: relative;
            }
            
            /*-------------------------------------------------CARD-------------------------------------------------*/
            
            
            .blog-card {
                position: relative;
                font-family: "Open Sans", sans-serif;
                color:#fff;
                right: 0;  
                left: 0;
                overflow: hidden;
                border-radius: 10px;
                box-shadow: 0px 10px 20px -9px rgba(0, 0, 0, 0.5);
                text-align: center;
                transition:all 0.4s;
                width: 43vw;
                height: 64vh;
                margin-top:-3vh;
                background: rgba(255, 255, 255, 0.2);
            }
            .blog-card:nth-child(3){
                width: 90.9vw;
                height: 10vh;  
                margin: 0 auto;
                margin-top:-8%;
                text-align: left;
                background: linear-gradient(rgba(0,0,0,.5), rgba(0,0,0,.5)), url(https://static.vecteezy.com/system/resources/previews/001/849/461/non_2x/soft-flat-abstract-shape-background-free-vector.jpg) center no-repeat;
            }
            .blog-card h3:nth-child(3){
                margin-top: 0;
            }
            .blog-card a{ 
                color:#fff;
                text-decoration:none;
                transition:all 0.2s
            }
            .blog-card .color-overlay {
                box-shadow: inset 0 0 0 200px rgba(255,255,255,0.05);
                filter: blur(30px);
                background: rgba(0, 0, 0, 0.3);
                opacity: 0.8;
                width: 43vw;
                height: 64vh;
                position: absolute;
                z-index: 10;
                top: 0;
                left: 0;
                transition: background 0.3s cubic-bezier(0.33, 0.66, 0.66, 1);
            }

            .blog-card .gradient-overlay {  
                background-image: linear-gradient(transparent 0%, rgba(0, 0, 0, 0.6) 21%);
                width: 43vw;
                height: 64vh;
                position: absolute;
                top: 32.41vh;
                left: 0;
                z-index: 15;
            }
            .blog-card:hover{
                box-shadow: 0px 18px 20px -9px rgba(0, 10, 30, 0.75);
                transform: scale(1.03);
                background-size: 150%;
            }
            .blog-card:hover .card-info {
                opacity: 1;
                bottom: 20vh;
            }
            .blog-card:hover .color-overlay {
                background: rgba(64, 64, 70,0.8);
            }
            .blog-card:hover .title-content{
                margin-top:11vh
            }
            .title-content {
                text-align: center;
                margin: 170px 0 0 0;
                position: absolute;
                z-index: 20;
                width: 100%;
                top: 0;
                left: 0;
                transition:all 0.6s
            }
            
            .blog-card:hover h3:after{
            
                animation: changeLetter 0.3s 1 linear;
                width:80%
            }
            
            .blog-card h3,h1 {
                font-size: 3vw;
                font-weight: 400;
                letter-spacing: 1px;
                font-family: "Open Sans", sans-serif;
                margin-bottom: 0;
                display:inline-block;
                width: 80%;
            }
            
            .blog-card h3{
                margin-top:-7%;
            }
            h3.reg{
                margin-top:0;
            }
            /* .blog-card h3 a:hover{
                text-shadow: 0px 8px 20px rgba(0, 0, 0, 0.95);
            } */
            h3:after {
                content: " ";
                display: block; 
                width: 10%;
                height: 2px;
                margin: 20px auto;
                border: 0;
                background: #a9a9a9;
                transition:all 0.2s
            }
            
            @keyframes changeLetter {
                0% {
                width: 10%;
                }
                100% {
                width: 80%;
                }
            }
            
            .intro {
                width: 170px;
                margin: 0 auto;
                color: #ddd;
                font-style: italic;
                line-height: 18px;
            }
            .intro a:hover{
                text-decoration:underline
            }
            .card-info {
                box-sizing: border-box;
                padding: 0;
                width: 100%;
                position: absolute;
                bottom: -40px;
                left: 0;
                margin: 0 auto;
                padding: 0 50px;
                font-size: 1.3vw;
                line-height: 30px;
                z-index: 20;
                opacity: 0;
                transition: bottom 0.64s, opacity 0.63s cubic-bezier(0.33, 0.66, 0.66, 1);
            }
            
            .utility-info {
                position: absolute;
                bottom: 0px;
                left: 0;
                z-index: 20;
                width:100%;
                text-align:left
            }
            .utility-info:after{
                content:" ";
                background: url(https://rawcdn.githack.com/Nodws/NodPen/ffad95aa5244b4b09a3c7c1508a018959bbedb7e/postItem/licons.svg) center no-repeat;
                background-size: 30px auto;
                display:block;
                opacity: 0.4;
                position:absolute;
                bottom:25px;
                right:15px;
                width:30px;
                height:15px
            }
            .utility-list {
                list-style-type: none;
                margin: 0 0 10px 20px;
                padding: 0;
                width: 100%;
            
            }
            .utility-list li {
                margin: 0 5px 0 0;
                padding: 3px 0 15px 0px;
                display: inline-block;
                
                font-size:0.8em
            }
            
            .licon{
                position:relative;  
                width:23px;
                height:15px;
                display:inline-block;
                vertical-align:middle;
            }
            .licon:before{
                content:"";
                background:url(https://rawcdn.githack.com/Nodws/NodPen/ffad95aa5244b4b09a3c7c1508a018959bbedb7e/postItem/licons.svg?) -2px -6px no-repeat;
                background-size:250px;
                width:26px;
                height:20px;
                display:inline-block;
                vertical-align:middle;
                position:absolute;
                top:-3px;
                left:0;
            }
            .icon-white{
                filter: brightness(10);
            }
            .icon-black{
                filter: brightness(0);
                opacity:0.6
            }
            .icon-like:before{
                background-position: -183px -6px;
            
            }
            .icon-com:before{
                background-position: -63px -4px;
            
            }
            .icon-dat:before{
                background-position: -94px -7px;
            }
            .icon-tag:before{
                background-position: -33px -6px;
            
            }
            
            @media (max-width:750px)
            {
                .utility-info {
                text-align:center;
                }
                .utility-info ul{
                width:100%;
                margin:0;
                box-sizing:border-box
                }
                .utility-info li{
                width:49%;
                display:inline-block;
                box-sizing:border-box;
                margin:0
                }
            }
            
            @media (max-width:500px){
                .utility-info li:last-of-type{
                width:100%
                }
                .card-info{
                display:none
                }
