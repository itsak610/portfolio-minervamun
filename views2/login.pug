            /*------------------ PRELOADER ------------------*/

            .loader {
                position: absolute;
                z-index: 999999;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                background: white;
                display: flex;
                justify-content: center;
                align-items: center;
                background: #080808;
            }


            .loader.hidden {
                transition: all 1s ease-in-out;
                animation: fadeOut 0.5s;
                animation-fill-mode: forwards;
            }
            @keyframes fadeIn {
                0%{
                    opacity: 0;
                }
                50% {
                    opacity: 0.9;
                }
                100% {
                    opacity: 1;
                }
            }
            @keyframes fadeOut {
                from {
                    opacity: 1;
                }
                to {
                    opacity: 0;
                    visibility: hidden;
                }
            }

            .loader {
                max-width: 100vw;
                background: #080808;
                width: 100%;
                height: 100%;
                position: fixed;
                display: flex;
            }

            
            .loadcontainer {
                width: 100%;
                height: 100%;
                color: #222;
                overflow: hidden;
            }

            .side {
                position: relative;
                display: flex;
                justify-content: center;
                align-items: center;
                top: 50%;
                transform: translate(0, -50%);
            }
            .side > * {
                background-color: #a9a9a9;
                animation: bottom 1.5s ease infinite;
                position: relative;
                max-width: 100%;
                max-height: 100%;
                width: 25px;
                height: 25px;
                border-radius: 100px;
                margin:20px;
            }
            .two{
            animation-delay:0.2s
            }
            .three{
            animation-delay:0.4s
            }

            @keyframes bottom {
                0% {
                    bottom: 0vw;
                }
                25% {
                    bottom: 0vw;
                }
                50% {
                    bottom: 5vw;
                }
                75% {
                    bottom: 0vw;
                }
                100% {
                    bottom: 0vw;
                }
            }

            .outro{
            animation: exit 0.3s;
            animation-delay : 0.5s;
            animation-fill-mode: forwards;
            }
            @keyframes exit {
                from {
                    margin-top: 0vh;
                    display: block;
                }
                to {
                    margin-top: 100vh;
                    display: none;
                }
            }
            @media (max-width:750px) {
                .side > * {
                    height: 12px;
                    width: 12px;
                    margin:10px;
                }   
            }
            @media (min-width:750px) and (max-width:1112px) {
                .side > * {
                    height: 16px;
                    width: 16px;
                    margin:12px;
                }   
            }


            
  
  
