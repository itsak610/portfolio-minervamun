            /*----------------------------------------------GLOBAL----------------------------------------------*/
            html {
                overflow: hidden;
                width: 100%;
            }
            
            body {
                height: 100%;
                width: 100%;
                position: fixed;
                /* prevent overscroll bounce*/
                background-color: lightgreen;
                overflow-y: scroll;
                -webkit-overflow-scrolling: touch;
                /* iOS velocity scrolling */
            }
              header > *{
                -webkit-tap-highlight-color:transparent;
                -webkit-tap-highlight-color:transparent;
                -moz-user-select: none;
                -khtml-user-select: none;
                -webkit-user-select: none;
                -ms-user-select: none;
                user-select: none;
            }
            section{
                margin-top: 10vw;
            }
            * {
            margin: 0;
            padding: 0;
            font-family: "Barlow";
            cursor: none;
            }
            body {
            color: #fff;
            overflow-x: hidden;
            background: #080808;
            }

            ::-webkit-scrollbar {
            width: 13px;
            z-index: -1;
            cursor: crosshair;
            }

            ::-webkit-scrollbar-thumb {
            border: 5px solid transparent;
            background-clip: content-box;
            border-radius: 100px;
            background-color: #fff;
            }
            ::-webkit-scrollbar-track {
            background: darkgray;
            border: 7px solid transparent;
            background-clip: content-box;
            }

            ::selection {
            background-color: #fff;
            color: #000;
            }

            ul {
            list-style-type: none;
            }

            ul li {
            list-style-type: none;
            }

            a {
            text-decoration: none;
            color: #fff;
            }

            #canvas {
            position: fixed;
            z-index: -1;
            opacity: 0.5;
            /* margin-left:-10vh; */
            }
            .mob-logo{
                display:none
            }
            .temp-body {
                opacity: 0.05;
                transition: all 0.2s ease-in-out;
            }
            /*----------------------------------------------CURSOR----------------------------------------------*/

            #cursor {
            position: fixed;
            z-index: 9999;
            left: 0;
            top: 0;
            pointer-events: none;
            will-change: transform;
            mix-blend-mode: difference;
            }

            @media (hover: hover) and (pointer: fine) {
            .cursor__circle {
                width: 64px;
                height: 64px;
                margin-top: -50%;
                margin-left: -50%;
                border-radius: 50%;
                border: solid 1px rgba(255, 255, 255, 1);
                transition: opacity 0.3s cubic-bezier(0.25, 1, 0.5, 1),
                background-color 0.3s cubic-bezier(0.25, 1, 0.5, 1),
                border-color 0.3s cubic-bezier(0.25, 1, 0.5, 1),
                width 0.3s cubic-bezier(0.25, 1, 0.5, 1),
                height 0.3s cubic-bezier(0.25, 1, 0.5, 1);
            }

            #cursor.arrow .cursor__circle {
                width: 128px;
                height: 128px;
                background-color: rgba(256, 256, 256, 1);
                border-color: white;
            }

            #cursor.arrow::after {
                content: "";
                position: absolute;
                left: -50%;
                top: -50%;
                width: 100%;
                height: 100%;
                /* background-image: url('https://svgshare.com/i/Yis.svg');
                    background-size: contain;
                    background-repeat: no-repeat;
                    background-position: center; */
            }

            #cursor.subtle .cursor__circle {
                opacity: 0.16;
            }

            #cursor.overlay .cursor__circle {
                width: 48px;
                height: 48px;
                background-color: rgba(227, 222, 193, 0.08);
                border-color: transparent;
            }
            }

            /*----------------------------------------------HEADER----------------------------------------------*/
            header {
            display: flex;
            position:absolute;
            justify-content: space-between;
            align-items: center;
            height: 100px;
            font-size: 20px;
            background-color: transparent;
            width: 100%;
            z-index:999;
            }

            .logo img {
            padding-left: 20px;
            height: 40px;
            width: auto;
            transform: rotate(1deg);
            transition: 0.3s ease;
            }
            .logo img:hover {
            transform: scale(1.1) rotate(1deg);
            }

            .navbar {
            width: 50vw;
            }

            .navbar ul {
            display: flex;
            flex-flow: row;
            justify-content: space-evenly;
            }

            .navbar ul li {
            padding: 5px 15px;
            border: 1px transparent solid;
            transition: 0.3s ease;
            }

            .navbar ul li:hover {
            transform: scale(1.2);
            }

                        /*----------------------------------------------SECTION----------------------------------------------*/

            section {
            height: 100vh;
            width: 100%;
            display: flex;
            justify-content: center;
            align-items: center;
            }

            .containerCard {
            position: absolute;
            height: 100%;
            width:100%;
            display: flex;
            align-items: center;
            flex-flow: column;
            }

            .row1 {
            display: flex;
            flex-flow: row;
            height: 40%;
            width: 100%;
            align-items: flex-end;
            justify-content: space-evenly;
            }

            .row2 {
            display: flex;
            flex-flow: row;
            width: 100%;
            height: 30%;
            align-items: center;
            justify-content: space-evenly;
            }

            .card {
            display: flex;
            padding: 20px;
            width: 40%;
            justify-content: center;
            align-items: center;
            box-shadow: 20px 20px 50px rgba(0, 0, 0, 0.5);
            background: rgba(255, 255, 255, 0.1);
